{
  "manifest_version": 2,

  "name": "Prototype",
  "description": "Paw integration proof of concept",
  "version": "1.0",
"content_security_policy": "script-src 'unsafe-eval' 'self'; object-src 'self'",
"content_scripts":[{
    "matches":["https://*/*","http://*/*"],
   "js": ["siteScript/jquery-2.1.4.min.js","siteScript/bootstrap/js/bootstrap.js","siteScript/jquery-ui-1.11.4/jquery-ui.min.js","siteScript/FloatingLabels.js","siteScript/transparency.js","siteScript/plates.js","siteScript/mori.js","siteScript/immutable.js","siteScript/underscore-observe-master/demo/js/underscore.js","siteScript/underscore-observe-master/demo/js/json2.js","siteScript/underscore-observe-master/underscore-observe.js","siteScript/jquery.directive.js-master/jquery.directive.js","bower_components/angular/angular.js","bower_components/angular-hotkeys/src/hotkeys.js","bower_components/mousetrap/mousetrap.min.js","bower_components/q/q.js","loadPopup.js","src/ViewportRelative.js","src/Train.js","src/WheelEventSimulator.js","src/Gestures.js","src/Paw.js","siteScript/loadControlModule.js"],
   "css":["siteStyle/style.css","siteScript/jquery-ui-1.11.4/jquery-ui.theme.min.css","siteScript/jquery-ui-1.11.4/jquery-ui.structure.min.css","siteScript/jquery-ui-1.11.4/jquery-ui.min.css","siteStyle/FloatingLabels.css","siteScript/bootstrap/css/bootstrap.min.css"],
   "run_at":"document_start" 
}],
  "permissions": [
    "activeTab",
   "https://*/*",
    "http://*/*",
    "tabs",
    "<all_urls>"
    
  ],
  "web_accessible_resources":["popup.html"]
  
}

